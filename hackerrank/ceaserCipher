#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int ischaracter(char c){
    if ((c>='A' && c<='Z')||(c>='a'&&c<='z'))
        return 1;
    else
        return 0;
}
int main(){
    int n;
    cin >> n;
    string s;
    cin >> s;
    int k;
    cin >> k;
    int i=0;
    for(i=0;i<=n;i++)
    {
       if (ischaracter(s[i])){
           s[i]=s[i]+(k%26);
           if (s[i]>'z')
               s[i]='a'+(s[i]%122);
           else if(s[i]>'Z'){
               s[i]='A'+(s[i]%90);
           }
       }
        
       else
           continue;
    }
    cout << s;
    return 0;
}
