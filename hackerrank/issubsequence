#include <bits/stdc++.h>

using namespace std;

int main(){
    int q;
    cin >> q;
    string H="hackerrank";
    
    for(int a0 = 0; a0 < q; a0++){
        string s;
        cin >> s;
        int i,j=0;
        int len=s.length();
        for(i=0;i<len;i++){
            if(s[i]==H[j]) j++;
        }
        if(H[j]=='\0') cout <<"YES\n";
        else cout <<"NO\n";
        
        // your code goes here
    }
    return 0;
}
